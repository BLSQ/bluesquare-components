{
    "name": "bluesquare-components",
    "version": "0.1.0",
    "description": "reusable UI components for Bluesquare apps",
    "author": "Bluesquare",
    "license": "Apache-2.0",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "fix-lint": "prettier-eslint \"./src/**/*.js\" --lint-different  --write",
        "show-lint-problems": "eslint \"./src/**/*.js\"",
        "clean": "rm -rf dist",
        "compile": "npm run clean && NODE_ENV=production babel src --out-dir dist --ignore './src/stories/*,./src/components/**/*.stories.js' --copy-files --no-copy-ignored",
        "serve": "npm run clean && webpack --mode=production ",
        "build-local": "npm run compile && npm pack",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BLSQ/bluesquare-components.git"
    },
    "keywords": [],
    "bugs": {
        "url": "https://github.com/BLSQ/bluesquare-components/issues"
    },
    "homepage": "https://github.com/BLSQ/bluesquare-components#readme",
    "devDependencies": {
        "@babel/cli": "^7.14.3",
        "@babel/core": "^7.13.10",
        "@babel/preset-env": "^7.13.10",
        "@babel/preset-react": "^7.12.13",
        "@date-io/moment": "^1.3.13",
        "@storybook/addon-actions": "^6.3.0-beta.1",
        "@storybook/addon-essentials": "^6.3.0-beta.1",
        "@storybook/addon-links": "^6.3.0-beta.1",
        "@storybook/addon-postcss": "^2.0.0",
        "@storybook/builder-webpack5": "^6.3.0-beta.1",
        "@storybook/manager-webpack5": "^6.3.0-beta.1",
        "@storybook/react": "^6.3.0-beta.1",
        "babel-loader": "^8.2.2",
        "css-loader": "^5.1.3",
        "eslint": "^7.25.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^8.0.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.14.2",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-webpack-plugin": "^2.5.4",
        "fetch-mock": "^7.3.3",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.3.1",
        "mini-css-extract-plugin": "^1.6.0",
        "mocha": "^8.2.0",
        "mock-require": "^3.0.3",
        "nock": "^13.0.4",
        "prettier": "2.3.0",
        "prettier-eslint": "^12.0.0",
        "prettier-eslint-cli": "^5.0.0",
        "sinon": "^9.2.1",
        "storybook-addon-material-ui": "*",
        "style-loader": "^2.0.0",
        "webpack": "^5.27.0",
        "webpack-cli": "^4.5.0",
        "webpack-dev-server": "^3.11.2"
    },
    "peerDependencies": {
        "@material-ui/core": "^4.11.4",
        "@material-ui/icons": "^4.2.1",
        "@material-ui/pickers": "^3.2.10",
        "classnames": "^2.3.1",
        "color": "^3.1.2",
        "moment": "^2.20.1",
        "prop-types": "^15.7.2",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-intl": "^5.10.16",
        "react-router": "^3.0.0",
        "react-select": "~1.2.1"
    }
